<!-- 一覧、追加と詳細ができる -->
<div class="top">
    <ul class="todo_items">
        <% @todos.each do |k, todo| %>
            <li class="todo_item"><a href="/todo/items/<%= k %>"><%= todo["title"] + " : " + todo["content"] %></a></li>
        <% end %>
    </ul>
    <a href="/todo/new" class="button__default">追加</a>
    <%= @reqdata %> <!-- デバッグ表示用 -->
</div>
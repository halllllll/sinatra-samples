<!-- 一覧、追加と詳細ができる -->
<div class="top">
    <div>
        <a href="/todos/new" class="button--add button__decorate">追加</a>
    </div>
    <div class="todo__list">
        <ul class="todo__items">
            <% @todos.each do |k, todo| %>
                <%= gen_list(k, todo) %>
            <% end %>
        </ul>
    </div>
    <div class="">
        <%= @reqdata %> <!-- デバッグ表示用 -->
    </div>
</div>